package com.revature.employees;

public class Employee {
	int empId;
	String name;
	String department;
	double salary;
public void createEmployee(int empId, String name, String department, double salary){
	this.empId=empId;
	this.name=name;
	this.department=department;
	this.salary=salary;
}
	{
	
	}

}
